  "tokenColors": [
    // Token Colors - Adjusted for a light background
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#999999" // Medium grey for comments
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.language",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#007ACC" // Darker blue
      }
    },
    {
      "scope": ["entity", "entity.name", "entity.name.type", "entity.other"],
      "settings": {
        "foreground": "#228B22" // Darker green
      }
    },
    {
      "scope": ["entity.name.tag", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#C62828" // Darker red
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#C2185B" // Darker pink
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#FFA000" // Darker orange
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#228B22" // Darker green
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#007ACC" // Darker blue
      }
    },
    {
      "scope": [
        "keyword.control",
        "storage.type.function.arrow",
        "keyword.operator.new",
        "keyword.control.new"
      ],
      "settings": {
        "foreground": "#C2185B" // Darker pink
      }
    },
    {
      "scope": ["keyword.operator", "punctuation.operator"],
      "settings": {
        "foreground": "#444444" // Dark grey
      }
    },
    {
      "scope": ["markup.changed.git_gutter"],
      "settings": {
        "foreground": "#FFA000" // Darker orange
      }
    },
    {
      "scope": ["markup.inserted.git_gutter"],
      "settings": {
        "foreground": "#228B22" // Darker green
      }
    },
    {
